{
  "name": "Big Netflix",
  "version": "0.1",
  "manifest_version": 2,
  "description": "",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    { 
      "matches": ["http://*.netflix.com/*", "https://*.netflix.com/*"],
      "js": ["big-netflix.js"]
    }
  ],
  "web_accessible_resources": [
    "big-netflix.css"
  ]
}
